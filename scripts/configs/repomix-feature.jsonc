{
  "output": {
    "filePath": "ai/codebase.xml.txt",
    "headerText": "LogoScripta Codebase",
    "style": "xml",
    // "removeComments": true,
    "removeEmptyLines": true,
    // "compress": true
    "compress": false,
    "topFilesLength": 10,
    "fileSummary": true,
    "directoryStructure": true,
    "git": {
      "sortByChanges": true,
      "includeDiffs": false
    }
  },
  "tokenCount": {
    "encoding": "o200k_base"
  },
  "include": [
    "src/core/serialization/export.ts",
    "src/domain/models/content/**/*",
    "src/domain/models/tagging/**/*",
    "src/domain/services/content/import-export.ts",
    "src/domain/services/tagging/import-export.ts",
    "src/ui/components/content/import/*",
    "src/ui/components/extra/import-export/*",
    "src/ui/components/table/controls/data-export/*",
    "src/interfaces/backend/views/tagging/TaggingView.tsx",
    "src/interfaces/backend/views/content/ContentView.tsx"
  ]
}
